{
  "version": "1.14.0",
  "name": "achievers-admin",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm start\"",
    "start": "node ./server.js",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "seed": "prisma db seed",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "test": "vitest",
    "test:ci": "vitest run --silent --coverage",
    "test:watch": "vitest dev",
    "pretest:e2e:run": "npm run build",
    "test:e2e": "playwright test --project=chromium",
    "typecheck": "tsc",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.16.0",
    "@prisma/client": "^5.3.1",
    "@remix-run/express": "^1.19.3",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@types/source-map-support": "^0.5.7",
    "applicationinsights": "^2.7.3",
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "daisyui": "^3.7.4",
    "dayjs": "^1.11.9",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "iconoir-react": "^6.11.0",
    "isbot": "^3.6.13",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.5.1",
    "remix-auth-oauth2": "^1.9.0",
    "source-map-support": "^0.5.21",
    "tiny-invariant": "^1.3.1",
    "typescript": "^5.2.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.38.0",
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@remix-run/testing": "^1.19.3",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/compression": "^1.7.3",
    "@types/eslint": "^8.44.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.2",
    "@types/node-cron": "^3.0.8",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.4",
    "autoprefixer": "^10.4.15",
    "binode": "^1.0.5",
    "c8": "^8.0.1",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "msw": "^1.3.1",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "^5.3.1",
    "semantic-release": "^22.0.0",
    "tailwindcss": "^3.3.3",
    "tsconfig-paths": "^4.2.0",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.4"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duvet86/achievers-admin.git"
  }
}
