{
  "version": "1.174.2",
  "name": "achievers-admin",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "cross-env NODE_ENV=development node -r dotenv/config ./server.mjs",
    "start": "node ./server.mjs",
    "format": "prettier --write .",
    "lint": "eslint",
    "seed": "prisma db seed",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "test": "cross-env VITEST=true vitest",
    "test:ci": "prisma generate && vitest run --silent --coverage",
    "test:watch": "vitest dev",
    "pretest:e2e:run": "npm run build",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:ui": "playwright test --project=chromium --ui",
    "typecheck": "react-router typegen && tsc",
    "prepare": "husky"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry": "^1.14.2",
    "@azure/storage-blob": "^12.29.1",
    "@casl/ability": "^6.7.3",
    "@casl/prisma": "^1.5.2",
    "@lexical/react": "^0.38.2",
    "@mjackson/file-storage": "^0.7.0",
    "@mjackson/form-data-parser": "^0.9.1",
    "@prisma/adapter-mariadb": "^6.19.0",
    "@prisma/client": "^6.19.0",
    "@react-router/express": "^7.9.6",
    "@react-router/node": "^7.9.6",
    "@tailwindcss/vite": "^4.1.17",
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "classnames": "^2.5.1",
    "compression": "^1.8.1",
    "dayjs": "^1.11.19",
    "express": "^5.1.0",
    "iconoir-react": "^7.11.0",
    "isbot": "^5.1.32",
    "lexical": "^0.38.2",
    "morgan": "^1.10.1",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.6",
    "remix-auth": "^4.2.0",
    "remix-auth-oauth2": "^3.4.1",
    "source-map-support": "^0.5.21",
    "tiny-invariant": "^1.3.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@react-router/dev": "^7.9.6",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/source-map-support": "^0.5.10",
    "@vitest/coverage-v8": "^4.0.9",
    "cross-env": "^10.1.0",
    "daisyui": "^5.5.5",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^6.19.0",
    "tailwindcss": "^4.1.17",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.9",
    "vitest-mock-extended": "^3.1.0"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duvet86/achievers-admin.git"
  }
}
