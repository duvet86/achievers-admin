{
  "version": "1.30.1",
  "name": "achievers-admin",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm start\"",
    "start": "node ./server.mjs",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "seed": "prisma db seed",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "test": "vitest",
    "test:ci": "vitest run --silent --coverage",
    "test:watch": "vitest dev",
    "pretest:e2e:run": "npm run build",
    "test:e2e": "playwright test --project=chromium",
    "typecheck": "tsc",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.17.0",
    "@prisma/client": "^5.5.2",
    "@remix-run/express": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "applicationinsights": "^2.9.1",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "daisyui": "^3.9.4",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "iconoir-react": "^7.0.2",
    "isbot": "^3.7.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-oauth2": "^1.11.0",
    "source-map-support": "^0.5.21",
    "tiny-invariant": "^1.3.1",
    "typescript": "^5.2.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@faker-js/faker": "^8.2.0",
    "@playwright/test": "^1.39.0",
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@remix-run/testing": "^2.2.0",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/compression": "^1.7.5",
    "@types/eslint": "^8.44.7",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/source-map-support": "^0.5.10",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "msw": "^2.0.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.5.2",
    "semantic-release": "^22.0.7",
    "tailwindcss": "^3.3.5",
    "tsconfig-paths": "^4.2.0",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "vitest-mock-extended": "^1.3.1"
  },
  "engines": {
    "node": ">=20"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duvet86/achievers-admin.git"
  }
}
