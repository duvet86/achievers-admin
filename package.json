    "@lexical/react": "^0.20.0",
    "@mjackson/file-storage": "^0.3.0",
    "@mjackson/form-data-parser": "^0.5.0",
    "@prisma/client": "^6.0.0",
    "@react-router/express": "^7.0.1",
    "@react-router/node": "^7.0.1",
    "chart.js": "^4.4.6",
    "classnames": "^2.5.1",
    "compression": "^1.7.5",
    "dayjs": "^1.11.13",
    "express": "^4.21.1",
