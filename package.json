{
  "version": "1.172.4",
  "name": "achievers-admin",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "cross-env NODE_ENV=development node -r dotenv/config ./server.mjs",
    "start": "node ./server.mjs",
    "format": "prettier --write .",
    "lint": "eslint",
    "seed": "prisma db seed",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "test": "cross-env VITEST=true vitest",
    "test:ci": "prisma generate && vitest run --silent --coverage",
    "test:watch": "vitest dev",
    "pretest:e2e:run": "npm run build",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:ui": "playwright test --project=chromium --ui",
    "typecheck": "react-router typegen && tsc",
    "prepare": "husky"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry": "^1.11.1",
    "@azure/storage-blob": "^12.28.0",
    "@casl/ability": "^6.7.3",
    "@casl/prisma": "^1.5.2",
    "@lexical/react": "^0.35.0",
    "@mjackson/file-storage": "^0.7.0",
    "@mjackson/form-data-parser": "^0.9.1",
    "@prisma/adapter-mariadb": "^6.15.0",
    "@prisma/client": "^6.15.0",
    "@react-router/express": "^7.8.2",
    "@react-router/node": "^7.8.2",
    "@tailwindcss/vite": "^4.1.13",
    "chart.js": "^4.5.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "classnames": "^2.5.1",
    "compression": "^1.8.1",
    "dayjs": "^1.11.18",
    "express": "^5.1.0",
    "iconoir-react": "^7.11.0",
    "isbot": "^5.1.30",
    "lexical": "^0.35.0",
    "morgan": "^1.10.1",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.2",
    "remix-auth": "^4.2.0",
    "remix-auth-oauth2": "^3.4.1",
    "source-map-support": "^0.5.21",
    "tiny-invariant": "^1.3.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.55.0",
    "@react-router/dev": "^7.8.2",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/source-map-support": "^0.5.10",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "daisyui": "^5.1.7",
    "dotenv": "^17.2.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.15.0",
    "tailwindcss": "^4.1.13",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duvet86/achievers-admin.git"
  }
}
