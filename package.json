    "semantic-release": "^24.2.0",
    "tailwindcss": "^3.4.15",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.6",
    "vitest-mock-extended": "^2.0.2"
