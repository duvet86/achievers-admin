{
  "version": "1.149.0",
  "name": "achievers-admin",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "cross-env NODE_ENV=development node -r dotenv/config ./server.mjs",
    "start": "node ./server.mjs",
    "format": "prettier --write .",
    "lint": "eslint",
    "seed": "prisma db seed",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "test": "cross-env VITEST=true vitest",
    "test:ci": "prisma generate && vitest run --silent --coverage",
    "test:watch": "vitest dev",
    "pretest:e2e:run": "npm run build",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:ui": "playwright test --project=chromium --ui",
    "typecheck": "react-router typegen && tsc",
    "prepare": "husky"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry": "^1.11.0",
    "@azure/storage-blob": "^12.27.0",
    "@casl/ability": "^6.7.3",
    "@casl/prisma": "^1.5.1",
    "@lexical/react": "^0.30.0",
    "@mjackson/file-storage": "^0.6.1",
    "@mjackson/form-data-parser": "^0.7.0",
    "@prisma/client": "^6.6.0",
    "@react-router/express": "^7.5.1",
    "@react-router/node": "^7.5.1",
    "@tailwindcss/vite": "^4.1.4",
    "chart.js": "^4.4.9",
    "chartjs-plugin-datalabels": "^2.2.0",
    "classnames": "^2.5.1",
    "compression": "^1.8.0",
    "dayjs": "^1.11.13",
    "express": "^5.1.0",
    "iconoir-react": "^7.11.0",
    "isbot": "^5.1.26",
    "lexical": "^0.30.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.1",
    "remix-auth": "^4.2.0",
    "remix-auth-oauth2": "^3.4.0",
    "source-map-support": "^0.5.21",
    "tiny-invariant": "^1.3.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.52.0",
    "@react-router/dev": "^7.5.1",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.1",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/source-map-support": "^0.5.10",
    "@vitest/coverage-v8": "^3.1.1",
    "cross-env": "^7.0.3",
    "daisyui": "^5.0.27",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.6.0",
    "tailwindcss": "^4.1.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "vitest-mock-extended": "^3.1.0"
  },
  "engines": {
    "node": ">=20"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duvet86/achievers-admin.git"
  }
}
