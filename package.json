    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/source-map-support": "^0.5.10",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.6",
